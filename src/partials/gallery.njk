<!-- Gallery -->
<section class="section section-gallery" id="gallery">
    <div class="container">
        <h2>{{ gallery.heading }}</h2>
        <div class="grid">
            {% for image in gallery.images %}
            <div class="card"><img src="{{ image.src }}" alt="{{ image.alt }}"></div>
            {% endfor %}
        </div>
    </div>
</section>
